{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "DigitalPersonaService": "Debug"
    }
  },
  "AllowedHosts": "*",
  "ServiceSettings": {
    "Port": 5001,
    "ServiceName": "DigitalPersonaFingerprintService",
    "DisplayName": "Digital Persona Fingerprint Service",
    "Description": "Windows service for Digital Persona fingerprint scanner integration"
  },
  "DigitalPersona": {
    "SDKPath": "SDK",
    "DeviceTimeout": 30000,
    "QualityThreshold": 70,
    "EnableLogging": true
  }
}
